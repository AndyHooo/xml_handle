<?xml version='1.0' encoding='utf-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd             http://www.springframework.org/schema/task              http://www.springframework.org/schema/task/spring-task-3.0.xsd">
	<description>calendar task config</description>
	<bean class="com.ytwl.cms.asyn.task.CmsContentsGeneratorTask" id="cmsContentsGeneratorTask" />
	<bean class="com.ytwl.cms.asyn.task.NewStockCalendarCatchTask" id="newerStockCalendarCatchTask" />
	
	<bean class="com.ytwl.cms.asyn.task.JudgeNewStockTask" id="judgeNewStockTask" />
	 
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="economicsDataGeneratorTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="cmsContentsGeneratorTask" /><entry key="targetMethod" value="buildEconomicsData" /></map></property></bean>
		</property>
		<property name="cronExpression">   
             
            
             
            <value>0 55 9 * * ?</value>
            
             
        </property>   
	<property name="misfireInstruction" value="2" /></bean> 
	
	 
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="economicsEventGeneratorTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="cmsContentsGeneratorTask" /><entry key="targetMethod" value="buildEconomicsEvent" /></map></property></bean>
		</property>
		<property name="cronExpression">    
              
           
            
             <value>0 0/5 * * * ?</value>
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	
	 
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="gubaDataGeneratorTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="cmsContentsGeneratorTask" /><entry key="targetMethod" value="buildGubaData" /></map></property></bean>
		</property>
		<property name="cronExpression">   
              
            
             <value>00 00 18 * * ?</value> 
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	


	 
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="buildJinShiKXDataTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="cmsContentsGeneratorTask" /><entry key="targetMethod" value="buildJinShiKXData" /></map></property></bean>
		</property>
		<property name="cronExpression">   
             
            
             <value>00 00 18 * * ?</value>
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="buildNewStockCalendarsTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="cmsContentsGeneratorTask" /><entry key="targetMethod" value="buildNewStockCalendars" /></map></property></bean>
		</property>
		<property name="cronExpression">   
             
           
          
           <value>0 0/5 * * * ?</value>
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="buildNewerStockCalendarsTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="newerStockCalendarCatchTask" /><entry key="targetMethod" value="buildNewerStockCalendars" /></map></property></bean>
		</property>
		<property name="cronExpression">   
             
           <value>0 0/15 * * * ?</value>
          
           
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="judgeNewStockTaskQuartz">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="judgeNewStockTask" /><entry key="targetMethod" value="doJudgeNewStockTask" /></map></property></bean>
		</property>
		<property name="cronExpression">   
             <value>0 0 1 * * ?</value> 
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="monitorFinancialCalendarTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="financialCalendarMonitorTask" /><entry key="targetMethod" value="monitorCalendarNullValue" /></map></property></bean>
		</property>
		<property name="cronExpression">   
           <value>0 0 */3 * * ?</value>
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="newSrTradingTipsTiggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="newSrTradingTipsTask" /><entry key="targetMethod" value="sync" /></map></property></bean>
		</property>
		<property name="cronExpression">   
           <value>0 0 */3 * * ?</value>
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	
	<bean class="org.springframework.scheduling.quartz.CronTriggerFactoryBean" id="srTradingTipsHistoryBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailFactoryBean">
				<property name="durability" value="true" /><property name="requestsRecovery" value="true" /><property name="jobClass" value="com.andy.quartz.JobDetailBean" /><property name="jobDataAsMap"><map><entry key="targetObject" value="gtjaSrTradingTask" /><entry key="targetMethod" value="syncSrTradingTipsHistory" /></map></property></bean>
		</property>
		<property name="cronExpression">   
           <value>0 0/30 * * * ?</value>
        </property>   
	<property name="misfireInstruction" value="2" /></bean>
	
	
	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">  
		<property name="triggers">  
            <list>
				
				<ref bean="judgeNewStockTaskQuartz" />
				<ref bean="buildNewStockCalendarsTiggerBean" />
				<ref bean="buildNewerStockCalendarsTiggerBean" />
				<ref bean="monitorFinancialCalendarTiggerBean" />
				<ref bean="newSrTradingTipsTiggerBean" />
				<ref bean="srTradingTipsHistoryBean" />
			</list>  
		</property>  
	<property name="dataSource" ref="dataSource" /><property name="overwriteExistingJobs" value="true" /><property name="exposeSchedulerInRepository" value="true" /><property name="autoStartup" value="true" /><property name="startupDelay" value="10" /><property name="applicationContextSchedulerContextKey" value="applicationContextKey" /><property name="configLocation" value="quartz.properties" /><property name="jobFactory"><bean class="com.andy.quartz.AutoWiringSpringBeanJobFactory" /></property></bean>
</beans>